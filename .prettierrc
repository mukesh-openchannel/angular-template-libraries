{
    "tabWidth": 4,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "htmlWhitespaceSensitivity": "ignore",
    "printWidth": 140,
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true,
    "overrides": [
        {
            "files": ["*.html"],
            "options": {
                "singleQuote": false
            }
        }
    ],
    "quoteProps": "as-needed",
    "endOfLine": "auto"
}
