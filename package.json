{
  "name": "oc-angular-template-libraries",
  "version": "2.47.0-4",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prebuild": "npm run clean-css && npm run generate-mock-css",
    "postbuild": "npm run gather-components-styles && npm run exclude-mixins && npm run replace-relative-scss-urls",
    "ng": "./node_modules/.bin/ng",
    "start": "./node_modules/.bin/ng serve",
    "build": "./node_modules/.bin/ng build",
    "build-prod": "npm run build --prod",
    "build-components": "npm run build angular-common-components ",
    "build-services-watch": "./node_modules/.bin/ng build angular-common-services --watch",
    "build-components-watch": "./scripts/build-components/build.sh --watch",
    "build-components-storybook-watch": "./scripts/build-components/build-storybook.sh --watch",
    "test": "npm run test:lib",
    "test:lib": "jest --config projects/angular-common-components/jest.config.js --collectCoverage --runInBand",
    "test:watch": "jest --config projects/angular-common-components/jest.config.js --watch --coverage --collectCoverageFrom=\"src/**/*${npm_config_folderName}*/**/*.{ts,html}\" -- \"${npm_config_folderName}\"",
    "test:one-file": "jest --config projects/angular-common-components/jest.config.js --coverage --collectCoverageFrom=\"src/**/*${npm_config_folderName}*/**/*.{ts,html}\" -- \"${npm_config_folderName}\"",
    "sass-lint": "node_modules/.bin/sass-lint -c .scss-lint.yml",
    "lint": "./node_modules/.bin/ng lint",
    "e2e": "./node_modules/.bin/ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "copy-assets": "mkdir -p storybook-assets/assets && cp -r projects/angular-common-components/assets/img/ storybook-assets/assets/angular-common-components/ && cp -r node_modules/tinymce storybook-assets/tinymce",
    "storybook": "npm run copy-assets && start-storybook -p 6006 -s ./projects/angular-common-components,./storybook-assets",
    "storybook-update-and-run": "npm install && ./node_modules/.bin/ng build && npx --userconfig .npmrc sb@latest upgrade && npm run storybook",
    "storybook-withoutdocs": "start-storybook -p 6006 -s projects/",
    "copy-assets-for-static-storybook": "mkdir -p ./storybook-static/assets/angular-common-components && cp -r ./projects/angular-common-components/assets/img/* ./storybook-static/assets/angular-common-components && cp -r node_modules/tinymce storybook-static/tinymce",
    "build-storybook": "build-storybook && npm run copy-assets-for-static-storybook",
    "angular-common-components-pack": "cd dist/angular-common-components/ && npm pack",
    "angular-common-components-up-version": "cd projects/angular-common-components/ && npm --no-git-tag-version version prerelease",
    "angular-common-services": "cd dist/angular-common-services/ && npm pack",
    "angular-common-services-pack": "npm run build-prod angular-common-services && npm run angular-common-services",
    "angular-common-services-up-version": "cd projects/angular-common-services/ && npm --no-git-tag-version version prerelease",
    "build-compodoc-services": "cd projects/angular-common-services && ../../node_modules/.bin/compodoc -n 'Angular common services' -p ./tsconfig.compodoc.json -d ../../compodoc-static/services --hideGenerator ${npm_config_startOptions}",
    "build-compodoc-components": "cd projects/angular-common-components && ../../node_modules/.bin/compodoc -n 'Angular common components' -p ./tsconfig.compodoc.json -d ../../compodoc-static/components --hideGenerator ${npm_config_startOptions}",
    "start-compodoc-service-watch": "npm run build-compodoc-services --startOptions=\"-s -w -r 8801\"",
    "start-compodoc-components-watch": "npm run build-compodoc-components --startOptions=\"-s -w -r 8802\"",
    "gather-components-styles": "scss-bundle -c scss-bundle.config.json",
    "exclude-mixins": "sed -i.bak -n '/@mixin/,/^$/!p' dist/angular-common-components/assets/styles/general-styles.scss",
    "clean-css": "find ./projects/angular-common-components -type f -name \"*.css*\" | xargs rm -f",
    "generate-mock-css": "find ./projects/angular-common-components -type f -name \"*.html\" | sed 's/\\.html/\\.css/g' | xargs touch",
    "watch-scss": "npm run clean-css && sass --watch ./projects/angular-common-components/src/lib",
    "scss-to-css": "npm run clean-css && sass ./projects/angular-common-components/src/lib",
    "replace-relative-scss-urls": "sed -i 's|url(../../../../assets/img|url(../img|g' dist/angular-common-components/assets/styles/general-styles.scss"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^11.2.14",
    "@angular/animations": "^11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@compodoc/compodoc": "^1.1.15",
    "angular-svg-icon": "^11.1.2",
    "broadcast-channel": "^3.5.3",
    "canvas": "^2.8.0",
    "core-js": "^3.9.1",
    "jwt-decode": "^3.1.2",
    "ngx-color-picker": "^11.0.0",
    "ngx-embed-video": "^1.0.4",
    "ngx-image-cropper": "^3.3.5",
    "ngx-infinite-scroll": "^10.0.0",
    "ngx-owl-carousel-o": "^5.0.0",
    "ngx-spinner": "^10.0.1",
    "npm-force-resolutions": "^0.0.10",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "~6.6.3",
    "tinymce": "^5.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "^0.1102.14",
    "@angular-devkit/build-ng-packagr": "~0.900.7",
    "@angular-devkit/core": "^11.2.2",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@babel/core": "^7.10.5",
    "@ng-bootstrap/ng-bootstrap": "^9.0.0",
    "@ng-select/ng-select": "^6.1.0",
    "@storybook/addon-a11y": "6.3.7",
    "@storybook/addon-actions": "6.3.7",
    "@storybook/addon-backgrounds": "6.3.7",
    "@storybook/addon-controls": "6.3.7",
    "@storybook/addon-docs": "6.3.7",
    "@storybook/addon-links": "6.3.7",
    "@storybook/addon-storysource": "6.3.7",
    "@storybook/addon-viewport": "6.3.7",
    "@storybook/addons": "6.3.7",
    "@storybook/angular": "6.3.7",
    "@tinymce/tinymce-angular": "^4.2.1",
    "@types/chart.js": "^2.9.20",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.11.1",
    "autoprefixer": "^8.6.5",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.4.1",
    "browserslist": "4.16.5",
    "chart.js": "^3.3.2",
    "codelyzer": "^6.0.0",
    "glob-parent": "5.1.2",
    "isbot": "^3.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^27.2.0",
    "jest-preset-angular": "^9.0.0",
    "lodash": "^4.17.21",
    "merge": "2.1.1",
    "minimist": "1.2.3",
    "ng-packagr": "11.1.4",
    "npm-watch": "^0.10.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "sass-lint": "^1.13.1",
    "scss-bundle": "^3.1.2",
    "trim": "0.0.3",
    "ts-jest": "^27.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.0.7",
    "set-value": "4.1.0",
    "immer": "9.0.6",
    "prismjs": "1.25.0"
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "trim": "0.0.3",
    "browserslist": "4.16.5",
    "minimist": "1.2.3",
    "merge": "2.1.1",
    "set-value": "4.1.0",
    "immer": "9.0.6",
    "prismjs": "1.25.0"
  }
}
